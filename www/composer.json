{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3,<7",
		"silverstripe/framework": "3.2.x-dev",
		"silverstripe/siteconfig": "3.2.x-dev",
		"gdmedia/ss-auto-git-ignore": "~0.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"scripts": {
		"post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
